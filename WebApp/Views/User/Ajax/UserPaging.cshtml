<table class="table">
    <thead>
        <tr>
            <th class="head0 nosort" scope="col" data-sortable="" style="width: 5%;">
                <input type="checkbox" name="" id="selectAll" />
            </th>
            <th scope="col">STT</th>
            <th scope="col">Tên Người dùng</th>
            <th scope="col">Email</th>
            <th scope="col">Số điện thoại</th>
            <th scope="col">Ngày lập </th>

        </tr>
    </thead>
    <tbody>
        @{
            var stt = 1 + (int)ViewBag.Stt;
            foreach (var item in Model)
            {
                <tr>
                    <td><input type="checkbox" class="ads_Checkbox" value="@item.Id" /></td>
                    <th scope="row">@stt</th>
                    <td><a href="/Company/Edit?Id=@item.Id">@item.UserName</a></td>
                    <td>@item.Email</td>
                    <td>@item.PhoneNumber</td>
                    <td>@item.CreatedDateUtc</td>
                </tr>
                stt++;
            }

        }

    </tbody>
</table>